{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "outDir": "_build_es6",
    "moduleResolution": "node",
    "sourceMap": false,
    "declaration":true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowJs":false,
    "skipLibCheck": true
  },
  "include": [
    "*.ts"
  ],
  "exclude":[
    "node_modules/**/*",
    "**/*.d.ts"
  ]
}
